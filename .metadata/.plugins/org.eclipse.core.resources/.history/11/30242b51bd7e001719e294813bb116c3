    CANNOT_CREATE_PERMANENT_DIRECTORY,
    CANNOT_MOVE_FILE,
    CANNOT_READ_LANGUAGE_FROM_FILE_NAME,
    CANNOT_READ_APPLICATION_NAME_FROM_FILE_NAME,
    CANNOT_READ_FILE,
